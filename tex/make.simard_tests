NAME = simard_tests

all: $(NAME).pdf

$(NAME).pdf: $(NAME).dvi
	dvips -Ppdf -N0 -o $(NAME).ps $(NAME).dvi
	ps2pdf $(NAME).ps
	rm $(NAME).ps

$(NAME).dvi: $(NAME).tex ../../ccpp-latex/ccpp.bib \
	latex $(NAME); bibtex $(NAME); latex $(NAME); latex $(NAME)

clean:
	rm -f *.pdf *.dvi 

fullclean:
	rm -f *.pdf *.log *.dvi *.aux *.bbl *.blg

dummy:
